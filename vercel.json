{  "headers": [    {      "source": "/index.html",      "headers": [        { "key": "Cache-Control", "value": "no-store" }      ]    },    {      "source": "/assets/(.*)",      "headers": [        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }      ]    }  ],  "rewrites": [    { "source": "/(.*)", "destination": "/index.html" }  ]} 
